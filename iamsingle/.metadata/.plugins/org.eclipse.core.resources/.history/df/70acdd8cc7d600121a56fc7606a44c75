package shaotian.android.iamsingle.netsdk;



public class LocParam implements INetParam{
	public double userid,altitude, latitude, longtitude=0;
	public String ret;
	
	public LocParam (int uid,double altitude,double latitude,double longtitude)
	{
		this.userid=uid;
		this.altitude=altitude;
		this.latitude=latitude;
		this.longtitude=longtitude;
	}
}
